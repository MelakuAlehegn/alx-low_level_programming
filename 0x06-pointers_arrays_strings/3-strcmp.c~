#include "main.h"
/**
 * *_strncpy - copies the strig
 *@dest: Destnation
 *@src: Source
 *@n: integer
 *Return: dest
 */
int _strcmp(char *s1, char *s2)
{
	int i;

	for (i = 0; s1[i] != '\0' && s2[i] != '\0'; i++)
	{
	  if (s1[i] != s2[i])
	    {
	      return (s1[i] - s2[i]);
	    }
	}
	return (0);
}
